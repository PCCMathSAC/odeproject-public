<prefigure xmlns="https://prefigure.org"
             label="resonance-3">

<diagram dimensions="(300,300)" margins="25">
  <definition>f(x) =1/6*x*sin(12*x)</definition>
  <coordinates bbox="[0,-2.2,4*pi, 2.2]">
    <grid-axes xlabel="t" ylabel="x(t)" h-pi-format="yes"/>

    <group at="graph-tangent">
      <graph at="graph" function='f' N="300" thickness="1"/>
    </group>
  </coordinates>

