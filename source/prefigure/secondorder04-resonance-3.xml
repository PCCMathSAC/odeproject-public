<diagram dimensions="(300,300)" margins="5">
  <definition>f(x) =1/6*x*sin(12*x)</definition>
  <coordinates bbox="[0,-2.2,4*pi, 2.2]">
    <grid-axes xlabel="t" ylabel="x(t)"/>

    <group at="graph-tangent">
      <graph at="graph" function='f' N="300"/>
    </group>
  </coordinates>

</diagram>
  
