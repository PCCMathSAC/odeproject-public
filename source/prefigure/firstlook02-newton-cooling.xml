<prefigure xmlns="https://prefigure.org"
             label="newton-cooling">
             <diagram dimensions="(300,300)" margins="5">
  <definition>f(x) = 28.6 * exp(-0.0725 * x) + 70</definition>
  <coordinates bbox="[-10,-10,50,110]">
    <grid-axes xlabel="t" ylabel="T(t)"/>

    <group at="graph-tangent">
      <graph at="graph" function='f' />
    </group>
  </coordinates>



</diagram>
</prefigure>
  
