<prefigure xmlns="https://prefigure.org"
             label="solution-dirac">
<diagram dimensions="(300,300)" margins="(50,5,5,5)">
  <coordinates bbox="(2,-0.15,6,0.15)">
    <grid-axes decorations="yes" xlabel="t" ylabel="y"/>
    <definition>
      f(t,y)=(y[1],-26*y[0] - 2*y[1] + delta(t,4))
    </definition>
    <plot-de-solution function="f" t0="0" t1="6" y0="(1,0)"/>
  </coordinates>
</diagram>
</prefigure>
  
