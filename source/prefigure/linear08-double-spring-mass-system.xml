<prefigure xmlns="https://prefigure.org"
             label="double-spring-mass-system">
<diagram dimensions="(1000,300)" margins="20">
  <coordinates bbox="(-2,-1,26,6)">
    <line endpoints="((-1,0),(26,0))"/>

    <line endpoints="((8,-1),(8,8))" dash="6 6" stroke="gray"/>
    <line endpoints="((16,-1),(16,8))" dash="6 6" stroke="gray"/>

    <rectangle center="(0,3)" dimensions="(3,6)"
	       fill="lightblue" stroke="black"/>
    <rectangle center="(8,1.5)" dimensions="(3,3)"
	       fill="lightblue" stroke="black"/>
    <rectangle center="(16,1.5)" dimensions="(3,3)"
	       fill="lightblue" stroke="black"/>
    <rectangle center="(24,3)" dimensions="(3,6)"
	       fill="lightblue" stroke="black"/>

    <path start="(1.5,1.5)" stroke="black" thickness="3">
      <lineto point="(6.5,1.5)" 
	      decoration="coil; number=6 ; dimensions=(6,6)"/>
    </path>

    <path start="(9.5,1.5)" stroke="black" thickness="3">
      <lineto point="(14.5,1.5)" 
	      decoration="coil; number=6 ; dimensions=(6,6)"/>
    </path>

    <path start="(17.5,1.5)" stroke="black" thickness="3">
      <lineto point="(22.5,1.5)" 
	      decoration="coil; number=6 ; dimensions=(6,6)"/>
    </path>

    <label anchor="(0,3)"><m>\text{wall}</m></label>
    <label anchor="(24,3)"><m>\text{wall}</m></label>

    <label anchor="(8,1.5)"><m>m_1</m></label>
    <label anchor="(16,1.5)"><m>m_2</m></label>

    <label anchor="(8,-1)" alignment="s"><m>x_1 = 0</m></label>
    <label anchor="(16,-1)" alignment="s"><m>x_2 = 0</m></label>

    <line endpoints="((8,5),(10,5))" stroke="black" thickness="3" arrows="1"/>
    <label anchor="(10,5)" alignment="e"><m>x_1</m></label>

    <line endpoints="((16,5),(18,5))" stroke="black" thickness="3" arrows="1"/>
    <label anchor="(18,5)" alignment="e"><m>x_2</m></label>
    


  </coordinates>
</diagram>
</prefigure>


