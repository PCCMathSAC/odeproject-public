<prefigure xmlns="https://prefigure.org"
             label="undamped-oscillator-phase-plane">

  <diagram dimensions="(300,300)" margins="5">
    <definition>f(t)=(2*cos(3*t) + (1/3)* sin(3*t),cos(3*t) - 6*sin(3*t))</definition>
    <coordinates bbox="(-2.5,-6.5,2.5,6.5)">
      <grid-axes xlabel="x(t)" ylabel="v(t)"/>
      <parametric-curve function="f" domain="(0,5)"
      N="600"/>
    </coordinates>

  <!--
      <annotations>
