<prefigure xmlns="https://prefigure.org"
             label="under-damped-oscillator-phase-plane">

<diagram dimensions="(300,300)" margins="5">
  <definition>f(t)=(exp(-0.2*t)*sin(t),cos(t)*e^(-0.2*t) - 0.2*e^(-0.2*t)*sin(t))</definition>
  <coordinates bbox="(-1.5,-1.5,1.5,1.5)">
    <grid-axes xlabel="x(t)" ylabel="v(t)"/>
    <parametric-curve function="f" domain="(0,20)"
          N="600"/>
  </coordinates>

