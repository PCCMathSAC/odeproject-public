<prefigure xmlns="https://prefigure.org"
             label="under-damped-oscillator-system">

<diagram dimensions="(400,300)" margins="25">
  <definition>f(t) = exp(-t) * sin(3*t)/3</definition>
  <definition>g(t) =  exp(-t)* cos(3*t) - exp(-t)*sin(3*t)/3</definition>
  <coordinates bbox="[0,-1,5,1]">
    <grid-axes xlabel="t" ylabel="x(t),v(t)"/>
    <graph at="position" function='f' />
    <graph at="velocity" function='g' stroke="red" />


    <legend at="legend" anchor="(bbox[2], bbox[3])" 
	    alignment="sw" scale="0.8" opacity="0.5">
      <item ref="position"><m>x(t) = (1/3)e^{-t} \sin 3t</m></item>
      <item ref="velocity"><m>v(t) = e^{-t}\cos(3t) - e^{-t}\sin(3t)/3</m></item>
    </legend>
  </coordinates>
</diagram>
</prefigure>


