<prefigure xmlns="https://prefigure.org"
             label="trace-determinant-repeated">

<diagram dimensions="(300,250)" margins="(35,35,50,35)">
  <coordinates bbox="(-4, -2, 4, 4)">
    <axes decorations="no">
      <xlabel><m>T</m></xlabel>
      <ylabel><m>D</m></ylabel>
    </axes>

    <definition>f(x)=x^2/4</definition>
    <graph function="f"/>

      <label anchor="(-0.5,3)" scale="0.8">
            <it>Center</it>
      </label>

      <label anchor="(1.2,3)" scale="0.8">
            <it>Spiral Source</it>
      </label>

      <label anchor="(-2.4,3)" scale="0.8">
            <it>Spiral Sink</it>
      </label>

      <label anchor="(-0.5,-1)" scale="0.8">
            <it>Saddle</it>
      </label>

      <label anchor="(2.8,1)" scale="0.8">
            <it>Nodal Source</it>
      </label>

      <label anchor="(-4,1)" scale="0.8">
            <it>Nodal Sink</it>
      </label>

      <label anchor="(4,4)" alignment="n">
            <m>D = T^2/4</m>
      </label>

      <label anchor="(1.8,2)" scale="0.8">
            <it>Source (unstable line)</it>
      </label>

      <label anchor="(-4,2)" scale="0.8">
            <it>Sink (stable line)</it>
      </label>

  </coordinates>
</diagram>

</prefigure>

