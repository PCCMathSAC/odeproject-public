<prefigure xmlns="https://prefigure.org"
             label="trace-determinant-repeated">

<diagram dimensions="(300,250)" margins="(35,35,50,35)">
  <coordinates bbox="(-4, -2, 4, 4)">
    <axes decorations="no">
      <xlabel><m>T</m></xlabel>
      <ylabel><m>D</m></ylabel>
    </axes>

    <definition>f(x)=x^2/4</definition>
    <graph function="f"/>

      <label anchor="(0,3)" scale="0.8">
            <it>Center</it>
      </label>

      <label anchor="(2.4,3)" scale="0.8">
            <it>Spiral Source</it>
      </label>

      <label anchor="(-2.4,3)" scale="0.8">
            <it>Spiral Sink</it>
      </label>

      <label anchor="(0,-1)" scale="0.8">
            <it>Saddle</it>
      </label>

      <label anchor="(3.5,1)" scale="0.8">
            <it>Nodal Source</it>
      </label>

      <label anchor="(-3.5,1)" scale="0.8">
            <it>Nodal Sink</it>
      </label>

      <label anchor="(4,4)" alignment="n">
            <m>D = T^2/4</m>
      </label>

      <label anchor="(2.8,2)" scale="0.8">
            <it>Source (unstable line)</it>
      </label>

      <label anchor="(-2.8,2)" scale="0.8">
            <it>Sink (stable line)</it>
      </label>

  </coordinates>

      <!--
      <annotations>
      <annotation ref="figure"
            text="A parabola D equals T squared divided by 4 in the TD plane.  On the D axis above the parabola are centers, above the parabola in the second quadrant are spiral sinks, above the axis in the first quadrant are spiral sources.  Nodal sinks are in the second quadrant below the parabola.  Nodal sources are in the first quadrant below the parabola.  Saddles are below the T axis.  The parabola in the second quadrant are stable lines. The parabola in the first quadrant are unstable lines."/>
    </annotations>
    -->


</diagram>

</prefigure>

