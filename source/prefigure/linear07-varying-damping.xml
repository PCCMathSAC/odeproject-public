<prefigure xmlns="https://prefigure.org"
             label="varying-damping">

<diagram dimensions="(300,250)" margins="(35,30,50,30)">
  <coordinates bbox="(-4, -2, 4, 4)">
    <axes decorations="no">
      <xlabel><m>T</m></xlabel>
      <ylabel><m>D</m></ylabel>
    </axes>

    <definition>f(x)=x^2/4</definition>
    <graph function="f"/>

      <label anchor="(-0.5,2)" scale="0.8">
            <it>Undamped</it>
      </label>

      <label anchor="(-2.4,3.4)" scale="0.8">
            <it>Under Dampled</it>
      </label>

      <label anchor="(-4.2,0.8)" scale="0.8">
            <it>Over Damped</it>
      </label>

      <label anchor="(4,4)" alignment="n">
            <m>D = T^2/4</m>
      </label>

      <label anchor="(-4,2)" scale="0.8">
            <it>Critically Damped</it>
      </label>

    <line endpoints="((0,3), (-4,3))" stroke="red"
    thickness="3" arrows="1"/>

    <point p="(0,3)" size="4" fill="red" stroke="none"></point>

      <label anchor="(0,3)" scale="0.8" alignment="e">
            <m>3</m>
      </label>

  </coordinates>
</diagram>

</prefigure>

