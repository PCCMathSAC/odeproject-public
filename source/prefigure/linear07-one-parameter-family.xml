<prefigure xmlns="https://prefigure.org"
             label="one-parameter-family">

<diagram dimensions="(300,250)" margins="(35,35,50,35)">
  <coordinates bbox="(-4, -2, 4, 4)">
    <axes decorations="no">
      <xlabel><m>T</m></xlabel>
      <ylabel><m>D</m></ylabel>
    </axes>

    <definition>f(x)=x^2/4</definition>
    <graph function="f"/>

      <label anchor="(0,3)" scale="0.8">
            <it>Center</it>
      </label>

      <label anchor="(-2.4,3)" scale="0.8">
            <it>Spiral Sink</it>
      </label>

      <label anchor="(-3,-1)" scale="0.8">
            <it>Saddle</it>
      </label>

      <label anchor="(-3,1)" scale="0.8">
            <it>Nodal Sink</it>
      </label>

      <label anchor="(4,4)" alignment="n">
            <m>D = T^2/4</m>
      </label>

      <label anchor="(-3,2)" scale="0.8">
            <it>Sink (stable line)</it>
      </label>

          <line endpoints="((-2,4), (-2,-2))" stroke="red"
    thickness="3"/>

      <label anchor="(-2,0)" scale="0.8" alignment="se">
            <m>-2</m>
      </label>

  </coordinates>

          <!--
      <annotations>
      <annotation ref="figure"
            text="A parabola D equals T squared divided by 4 in the TD plane.  On the D axis above the parabola are centers, above the parabola in the second quadrant are spiral sinks.  Nodal sinks are in the second quadrant below the parabola.  Sinks or stable lines are on the parabola in the second quadrant.  Saddles are below the T axis.  There is a vertical line through (-2,0)"/>
    </annotations>
    -->

</diagram>

</prefigure>

