<prefigure xmlns="https://prefigure.org"
             label="derivative-graphs-2">
             <diagram dimensions="(300,300)" margins="5">
  <definition>f(x) = (1/3)*(x + 3/2)*(x - 1)*(x - 5/2)^2</definition>
  <coordinates bbox="[-3,-6,3,6]">
    <grid-axes xlabel="y" ylabel="f(y)"/>

      <graph function='f' />
  
  </coordinates>

  <annotations>
    <annotation ref="figure"
      text="A fourth degree polynomial with roots at -3/2, 1 and a multiple root at 5/2"/>
  </annotations>

</diagram>
</prefigure>
  
