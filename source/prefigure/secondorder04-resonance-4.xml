<prefigure xmlns="https://prefigure.org"
             label="resonance-4">

<diagram dimensions="(300,300)" margins="35">
  <definition>f(x) =4/25*cos(13*x) + 4/25*cos(12*x)</definition>
  <coordinates bbox="[0,-0.4,10*pi, 0.4]">
    <grid-axes xlabel="t" ylabel="x(t)"  h-pi-format="yes"/>

    <group at="graph-tangent">
      <graph at="graph" function='f' N="400" thickness="1"/>
    </group>
  </coordinates>

</diagram>
  
</prefigure>