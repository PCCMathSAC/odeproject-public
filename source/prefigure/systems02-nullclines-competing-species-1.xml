<prefigure xmlns="https://prefigure.org"
             label="nullclines-competing-species-1">

<diagram dimensions="(300,300)" margins="25">
  <definition>f(t,y) = (y[0]*(1-y[0]) - 2*y[0]*y[1], y[1]*(1 - y[1]) - 2*y[0]*y[1])</definition>
  <coordinates bbox="[0,0,1.5,1.5]">
      <definition>gap = 1/8</definition>
    <grid-axes xlabel="x(t)" ylabel="y(t)"/>
    <slope-field function="f" system="yes" stroke="black"
     arrows="yes" thickness="1" spacings="((0,gap,1.5),(0,gap,1.5))"/>

    <plot-de-solution function="f" t0="0" t1="11" 
          y0="(1.4,1.3)" axes="(y0,y1)" arrow="yes" stroke="blue"/>

    <point p="(1.4,1.3)" size="4" fill="blue" stroke="none"></point>

    <line endpoints="((0,0), (0,1.5))" stroke="green"
    thickness="3"/>
    <line endpoints="((1,0), (0,0.5))" stroke="green"
    thickness="3"/>
    <line endpoints="((0,0), (1.5,0))" stroke="red"
    thickness="3"/>
    <line endpoints="((0.5,0), (0,1))" stroke="red"
    thickness="3"/>
  </coordinates>

    <!--
      <annotations>
      <annotation ref="figure"
            text="Direction field for x prime equals x times the quantity one minus x minus two times x times y and y prime equals y times the quantity one minus y minus two times x times y with a solution curve begins at the point (1.4, 1.3) and travels down and left and then curves back towards the point (1,0).  The x nullclines are given by the lines x equals zero and y equals one-half times the quantity one minus x.  The y nullclines are given by y equals zero and y equals one minus two times x."/>
    </annotations>
    -->
</diagram>
</prefigure>

