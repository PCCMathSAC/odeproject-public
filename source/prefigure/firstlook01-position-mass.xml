<prefigure xmlns="https://prefigure.org"
             label="position-mass">

             <diagram dimensions="(300,300)" margins="5">
  <definition>f(t)=sin(t)</definition>
  <coordinates bbox="(0,-1.5,30,1.5)">
    <grid-axes xlabel="t" ylabel="x(t)"/>

    <graph at="graph" function='f' />

  </coordinates>

      <annotations>
      <annotation ref="figure"
        text="A mass m attached by a spring to a wall and sliding back and forth on a frictionless surface"/>
    </annotations>
</diagram>

</prefigure>
