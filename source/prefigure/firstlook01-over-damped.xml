<prefigure xmlns="https://prefigure.org"
             label="over-damped">

             <diagram dimensions="(200,100)" margins="5">
  <definition>f(t)=exp(-t) - exp(-2*t)</definition>
  <coordinates bbox="(0,-0.1,10,0.3)">
    <grid-axes xlabel="t" ylabel="x(t)"/>

    <graph at="graph" function='f' />

  </coordinates>
</diagram>

</prefigure>
