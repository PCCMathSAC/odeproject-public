<prefigure xmlns="https://prefigure.org"
             label="modified-predator-prey-phase-plane-1">

<diagram dimensions="(300,300)" margins="25">
  <definition>f(t,y) = (2*y[0]*(1 - y[0]/10) - y[0]*y[1],-5*y[1] + y[0]*y[1])</definition>
  <coordinates bbox="[0,0,9,2.5]">
    <grid-axes xlabel="R(t)" ylabel="F(t)"/>

    <definition>gap = 1/2</definition>
    <slope-field function="f" system="yes" stroke="black"
     arrows="yes" thickness="1" spacings="((0,gap,9),(0,gap/2,2.5))"/>

    <plot-de-solution function="f" t0="0" t1="10" 
          y0="(0.5,2.5)" axes="(y0,y1)" arrow="yes" N="600"/>

  </coordinates>

  <!--
      <annotations>
      <annotation ref="figure"
            text="Direction field for R prime equals minus R times F  plus two times R times the quantity one minus R divided by ten  and F prime equals minus five times F plus R times F with a siolutioin curve that spirals towards the origin in a counterclockwise direction"/>
    </annotations>
  -->

</diagram>

</prefigure>

