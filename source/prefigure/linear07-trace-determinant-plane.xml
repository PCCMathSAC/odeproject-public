<prefigure xmlns="https://prefigure.org"
             label="trace-determinant-plane">

<diagram dimensions="(300,250)" margins="(35,30,50,30)">
  <coordinates bbox="(-4, -2, 4, 4)">
    <axes decorations="no">
      <xlabel><m>T</m></xlabel>
      <ylabel><m>D</m></ylabel>
    </axes>

    <definition>f(x)=x^2/4</definition>
    <graph function="f"/>

      <label anchor="(-1.14,2)" scale="0.8">
            <it>Complex Eigenvalues</it>
      </label>

      <label anchor="(-1.65,-1)" scale="0.8">
            <it>Distinct Real Eigenvalues</it>
      </label>

      <label anchor="(1.2,1.3)" scale="0.8">
            <it>Repeated Eigenvalues</it>
      </label>

      <label anchor="(4,4)" alignment="n">
            <m>D = T^2/4</m>
      </label>

  </coordinates>
</diagram>

</prefigure>

