<prefigure xmlns="https://prefigure.org"
             label="logistic-growth">
  <diagram dimensions="(300,300)" margins="50">
    <definition>f(x) = 1000/(9*exp(-0.8109 * x) + 1)</definition>
    <coordinates bbox="[0,0,20,1100]">
      <grid-axes xlabel="t" ylabel="P(t)"/>

      <graph at="graph" function='f' />

    </coordinates>

    <annotations>
      <annotation ref="figure"
        text="Integral curve of the differential equation P prime equal k times the quatity one minus P divided by 1000"/>
    </annotations>

  </diagram>
</prefigure>
