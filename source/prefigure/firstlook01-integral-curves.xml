<prefigure xmlns="https://prefigure.org"
             label="prefigure-rectangle2">

<diagram dimensions="(300,300)" margins="5">
  <definition>f1(x) = exp(x)/10</definition>
  <definition>f2(x) = exp(x)/20</definition>
  <definition>f3(x) = exp(x)/30</definition>
  <definition>f4(x) = -exp(x)/10</definition>
  <definition>f5(x) = -exp(x)/20</definition>
  <definition>f6(x) = -exp(x)/30</definition>
  <coordinates bbox="[-4,-4,4,4]">
    <grid-axes xlabel="t" ylabel="f(t)"/>

    <group at="solutions">
      <graph at="graph-f1" function='f1' />
      <graph at="graph-f2" function='f2' />
      <graph at="graph-f3" function='f3' />
      <graph at="graph-f4" function='f4' />
      <graph at="graph-f5" function='f5' />
      <graph at="graph-f6" function='f6' />
    </group>

    <label anchor="(-2,1)" alignment="center">
       <m>x(t) = Ce^{kt}</m>
    </label>
  </coordinates>

