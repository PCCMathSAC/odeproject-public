<prefigure xmlns="https://prefigure.org"
             label="example-1">
<diagram dimensions="(300,300)" margins="12">
  <definition>f(t,y) = (y[0] - 3*y[1] + y[0] * y[1]^2,2*y[0] - 4*y[1] - y[0]^2*y[1])</definition>
  <coordinates bbox="[-2,-2,2,2]">

    <grid-axes xlabel="x(t)" ylabel="y(t)"/>
    <slope-field function="f" system="yes" stroke="black"
     arrows="yes" thickness="1" spacings="((-2,1/4,2),(-2,1/4,2))"/>

    <plot-de-solution function="f" t0="0" t1="10" 
          y0="(1,1)" axes="(y0,y1)"/>
    <plot-de-solution function="f" t0="0" t1="2" 
          y0="(1,1)" axes="(y0,y1)" arrow="yes"/>

    <plot-de-solution function="f" t0="0" t1="6" 
          y0="(-0.5,1)" axes="(y0,y1)" stroke="red"/>
    <plot-de-solution function="f" t0="0" t1="0.8" 
          y0="(-0.5,1)" axes="(y0,y1)" stroke="red" arrow="yes"/>

    <point p="(1,1)" size="4" fill="blue" stroke="none"></point>
    <point p="(-0.5,1)" size="4" fill="red" stroke="none"></point>
  </coordinates>

        <!--
    <annotations>
      <annotation ref="figure"
      text="Phase portrait for x prime equals x minus 3 times y plus x times y squared and y prime equals 2 times x minus 4 times y minus x squared times y"/>
    </annotations>
    -->
</diagram>
</prefigure>

