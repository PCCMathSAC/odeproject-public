<prefigure xmlns="https://prefigure.org"
             label="rlc-circuit">

<diagram dimensions="(300,200)" margins="40">
  <coordinates bbox="(0,0,10,9)">
    <path start="(1,1)" stroke="black" thickness="1">
      <lineto point="(1,8)"/>
      <lineto point="(9,8)"
        decoration="zigzag; number=5; dimensions=(10,10)"/>
      <lineto point="(9,1)"
              decoration="capacitor; dimensions=(12,15)"/>
      <lineto point="(1,1)"
        decoration="coil; number=5; dimensions=(10,10)"/>
    </path>

    <label anchor="(0,4.5)" alignment="west">
      <m>E(t)</m>
    </label>

     <label anchor="(0,6)" alignment="east">
      <m>+</m>
    </label>

    <label anchor="(0,3)" alignment="east">
      <m>-</m>
    </label>

     <label anchor="(5,9)" alignment="center">
      <m>R</m>
    </label>

     <label anchor="(10,4.5)" alignment="center">
      <m>C</m>
    </label>

     <label anchor="(5,4.5)" alignment="center">
      <m>I(t)</m>
    </label>

     <label anchor="(5,0)" alignment="center">
      <m>L</m>
    </label>

    <point p="(1,4.5)" size="20" fill="white" stroke="black" thickness="1"/>

    <path start="(3.5,4)" stroke="black" arrows="1" thickness="1">
      <cubic-bezier controls="((4,6),(7,6.5),(6.5,4))"/>
    </path>

  </coordinates>
</diagram>

</prefigure>
