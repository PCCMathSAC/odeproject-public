<prefigure xmlns="https://prefigure.org"
             label="under-damped">

             <diagram dimensions="(300,300)" margins="5">
  <definition>f(t)=exp(-t)*sin(7*t)/7</definition>
  <coordinates bbox="(0,-0.1,10,0.15)">
    <grid-axes xlabel="t" ylabel="x(t)"/>

    <graph at="graph" function='f' />

  </coordinates>
</diagram>

</prefigure>
