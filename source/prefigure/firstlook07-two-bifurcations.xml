<prefigure xmlns="https://prefigure.org"
             label="bifurcation-two-bifurcations">

<diagram dimensions="(300,300)" margins="5">

  <definition>g(t)=(-t*(t-2)^2, t)</definition>
  <coordinates bbox="[-3,-0.5,3,3]">
    <grid-axes xlabel="\lambda" ylabel="y"/>

      <parametric-curve function="g" domain="(-1/2,3)"/>
  </coordinates>
