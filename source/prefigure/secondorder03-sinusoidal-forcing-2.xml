<prefigure xmlns="https://prefigure.org"
             label="sinusoidal-forcing-2">

<diagram dimensions="(300,300)" margins="25">
  <definition>b = 2</definition>
  <definition>k = 17</definition>
 <definition>f(t,y) = (y[1],-k*y[0] - b*y[1] - 2*sin(3*t))</definition>
  <coordinates bbox="[-0.25,-0.7,0.25,0.7]">
    <grid-axes xlabel="x(t)" ylabel="y(t)"/>

    <plot-de-solution function="f" t0="0" t1="10" 
          y0="(0,0)" axes="(y0,y1)" arrow="yes"   N="600"/>


  </coordinates>
</diagram>
</prefigure>
