<prefigure xmlns="https://prefigure.org"
             label="derivative-graphs-3">
             <diagram dimensions="(300,300)" margins="5">
  <definition>f(x) = -x^2*(x + 2)*(x - 2)/2+3</definition>
  <coordinates bbox="[-3,-4,3,6]">
    <grid-axes xlabel="y" ylabel="f(y)"/>

      <graph function='f' />
  
  </coordinates>

  <annotations>
    <annotation ref="figure"
      text="A fourth degree polynomial with roots at -2, 2 and a multiple root at 0"/>
  </annotations>

</diagram>
</prefigure>
  
