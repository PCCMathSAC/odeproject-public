<prefigure xmlns="https://prefigure.org"
             label="direction-field-example-3">

<diagram dimensions="(300,300)" margins="25">
  <definition>f(t,y) = (-y[0], -5*y[1])</definition>
  <coordinates bbox="[-3,-3,3,3]">
    <grid-axes xlabel="x(t)" ylabel="y(t)"/>

    <definition>gap = 1/2</definition>
    <slope-field function="f" system="yes" stroke="black"
     arrows="yes" thickness="1" spacings="((-3,gap,3),(-3,gap,3))"/>

  </coordinates>

      <!--
      <annotations>
      <annotation ref="figure"
            text="Direction field for x prime equals minus x and y prime equals minus five times y"/>
    </annotations>
    -->
</diagram>

</prefigure>
