<prefigure xmlns="https://prefigure.org"
             label="tailings-pond">

<diagram dimensions="(300,300)" margins="5">
  <definition>f(x) = (5/19)*(400 - x)*(1 - ((400 - x)/400)^19)</definition>
  <coordinates bbox="[0,-10,400,90]">
    <grid-axes xlabel="t" ylabel="x(t)"/>
    <graph at="graph" function='f' />
  </coordinates>

  <annotations>
    <annotation ref="figure" text="graph of the integral curve for dx over dt plus 20 times x over the quatity 400 minus t equals 5">
    </annotation>
  </annotations>

</diagram>

</prefigure>
  
