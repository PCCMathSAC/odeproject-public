<prefigure xmlns="https://prefigure.org"
             label="tailings-pond">

<diagram dimensions="(300,300)" margins="5">
  <definition>f(x) = (5/19)*(400 - x)*(1 - ((400 - x)/400)^19)</definition>
  <coordinates bbox="[0,-10,400,90]">
    <grid-axes xlabel="t" ylabel="x(t)"/>
    <graph at="graph" function='f' />
  </coordinates>

  <annotations>
    <annotation ref="figure" text="graph of a function">
      <annotation ref="graph" text="the graph starts at the origin then peaks and descends to zero" sonify="yes"/>
    </annotation>
  </annotations>

</diagram>

</prefigure>
  
