<prefigure xmlns="https://prefigure.org"
             label="bifurcation-pitchfork">

<diagram dimensions="(300,300)" margins="5">
  <definition>f(t)=0</definition>
  <definition>g(t)=(t^2,t)</definition>
  <coordinates bbox="[-1,-1,1,1]">
    <grid-axes xlabel="\alpha" ylabel="y"/>
      <graph function="f"/>
      <parametric-curve function="g" domain="(-2,2)"/>
  </coordinates>
<!--
  <annotations>
    <annotation ref="figure" text="graph of x equals y squared and the horizontal line y equals 0">
    </annotation>
  </annotations>
  -->
</diagram>

</prefigure>
  
