<prefigure xmlns="https://prefigure.org"
             label="lynx-snowshoe-hare">
             <diagram dimensions="(300,300)" margins="20">
  <definition>f(t,y) = (0.4*y[0] - 0.01*y[0]*y[1],-0.3*y[1] + 0.005*y[0]*y[1])</definition>
  <coordinates bbox="[-15,-15,70,85]">
    <grid-axes xlabel="t"/>

    <de-solve function="f" t0="0" t1="100" y0="(70,50)" name="oscillator"
	      N="200"/>
    <plot-de-solution name="Hgraph" solution="oscillator" axes="(t,y0)"/>
    <plot-de-solution name="Lgraph" solution="oscillator" axes="(t,y1)" stroke="red"/>

    <label anchor="(18,80)" alignment="e">
      <m>H(t)</m>
    </label>
    <label anchor="(50,30)" alignment="e">
      <m>L(t)</m>
    </label>

  </coordinates>
  <!--
    <annotations>
      <annotation ref="figure"
                text="Integral curves of the system of differential equation H prime equals 0.4 times H minus 0.1 times H times L and L prime equals minus 0.3 times L plus 0.005 times H times L"/>
    </annotations>
-->

</diagram>
</prefigure>


  
